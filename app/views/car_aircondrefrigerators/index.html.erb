<% content_for :content_header  do %>
  <section class="content-header">
    <h1><%=t('views.list')%> <small><%= t('activerecord.models.car_aircondrefrigerator') %></small></h1>

    <ol class="breadcrumb">
      <li><a href="/"><i class="fa fa-dashboard"></i> <%=t('views.home')%></a></li>
      <li class="active"><%= t('activerecord.models.car_aircondrefrigerator') %></li>
    </ol>
  </section>
<% end %>

<section class="content">
  <div class="row">
    <div class="col-xs-12">
      <div class="box box-primary">
        <div class="box-header">
          <div class="box-tools pull-right">
            <% if can? :create, CarAircondrefrigerator %>
              <%= link_to new_car_aircondrefrigerator_path(@car_aircondrefrigerator), :class => 'btn btn-success' do %>
                <span class="glyphicon glyphicon-plus"></span>
                <%=t('views.create')%><%=t('activerecord.models.car_aircondrefrigerator')%>
              <% end %>
            <% end %>
          </div>
        </div>
        <div class="box-body table-responsive">
          <table class="table table-hover">
            <thead>
              <tr>
                <th><%=t('activerecord.attributes.car_aircondrefrigerator.id') -%></th>

                <th><%=t('activerecord.attributes.car_aircondrefrigerator.car_model') -%></th>

                <th><%=t('activerecord.attributes.car_aircondrefrigerator.airconditioningcontrolmode') -%></th>

                <th><%=t('activerecord.attributes.car_aircondrefrigerator.tempzonecontrol') -%></th>

                <th><%=t('activerecord.attributes.car_aircondrefrigerator.rearairconditioning') -%></th>

                <th><%=t('activerecord.attributes.car_aircondrefrigerator.reardischargeoutlet') -%></th>
                <th></th>
                <th></th>
                <th></th>
              </tr>
            </thead>
            <tbody>
              <% current_page = @page.nil? ? 1 : @page -%>
              <% @page = @page.nil? ? 0 : (@page.to_i - 1) * 10 -%>
              <%= content_tag_for(:tr, @car_aircondrefrigerators) do |car_aircondrefrigerator| %>
                <td><%= @page = @page + 1   -%></td>
                <td><%= car_aircondrefrigerator.car_model.name -%></td>
                <td><%= car_aircondrefrigerator.airconditioningcontrolmode -%></td>
                <td><%= car_aircondrefrigerator.tempzonecontrol -%></td>
                <td><%= car_aircondrefrigerator.rearairconditioning -%></td>
                <td><%= car_aircondrefrigerator.reardischargeoutlet -%></td>
                <% if can? :read, car_aircondrefrigerator %>
                  <td class="list-opertion"><%= link_to t('views.show'), "#{car_aircondrefrigerator_path(car_aircondrefrigerator)}" %></td>
                <% end %>
                <% if can? :edit, car_aircondrefrigerator %>
                  <td class="list-opertion"><%= link_to t('views.edit'), edit_car_aircondrefrigerator_path(car_aircondrefrigerator) %></td>
                <% end %>
                <% if can? :destroy, car_aircondrefrigerator %>
                  <td class="list-opertion"><%= link_to t('views.destroy'), car_aircondrefrigerator, method: :delete, data: { confirm: t('views.confirm') } %></td>
                <% end %>

              <% end %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
    <%= paginate @car_aircondrefrigerators %>
</section>
