<section class="content invoice">
  <div class="row">
    <div class="col-md-10">
      <%= form_for(@car_basic, :html => { :role => "form" }) do |f| %>
        <% if @car_basic.errors.any? %>
          <div class="alert alert-danger alert-dismissable" role="alert">
            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">x</button>
            <h4><%= pluralize(@car_basic.errors.count, "error") %> prohibited this car_basic from being saved:</h4>

            <ul>
            <% @car_basic.errors.full_messages.each do |msg| %>
              <li><%= msg %></li>
            <% end %>
            </ul>
          </div>
        <% end %>

      <div class="form-group">
        <%= f.label :car_model %>
        <%= f.text_field :car_model, :class => "form-control" %>
      </div>
      <div class="form-group">
        <%= f.label :price %>
        <%= f.text_field :price, :class => "form-control" %>
      </div>
      <div class="form-group">
        <%= f.label :saleprice %>
        <%= f.text_field :saleprice, :class => "form-control" %>
      </div>
      <div class="form-group">
        <%= f.label :warrantypolicy %>
        <%= f.text_field :warrantypolicy, :class => "form-control" %>
      </div>
      <div class="form-group">
        <%= f.label :vechiletax %>
        <%= f.text_field :vechiletax, :class => "form-control" %>
      </div>
      <div class="form-group">
        <%= f.label :displacement %>
        <%= f.text_field :displacement, :class => "form-control" %>
      </div>
      <div class="form-group">
        <%= f.label :gearbox %>
        <%= f.text_field :gearbox, :class => "form-control" %>
      </div>
      <div class="form-group">
        <%= f.label :comfuelconsumption %>
        <%= f.text_field :comfuelconsumption, :class => "form-control" %>
      </div>
      <div class="form-group">
        <%= f.label :userfuelconsumption %>
        <%= f.text_field :userfuelconsumption, :class => "form-control" %>
      </div>
      <div class="form-group">
        <%= f.label :officialaccelerationtime100 %>
        <%= f.text_field :officialaccelerationtime100, :class => "form-control" %>
      </div>
      <div class="form-group">
        <%= f.label :maxspeed %>
        <%= f.text_field :maxspeed, :class => "form-control" %>
      </div>
      <div class="form-group">
        <%= f.label :seatnum %>
        <%= f.text_field :seatnum, :class => "form-control" %>
      </div>
      <div class="form-group">
        <%= f.submit :class => "btn btn-primary" %>
      </div>
    <% end %>
    </div>
  </div>
</section>
