            <dd class="well"><%= @car_seat.rearseatreclineproportion %></dd>
            <dt class="label-change">
            <span class="label-content label-info">
            <%=t('activerecord.attributes.car_seat.rearseatangleadjustment')%>:
            </span></dt>
            <dd class="well"><%= @car_seat.rearseatangleadjustment %></dd>
            <dt class="label-change">
            <span class="label-content label-info">
            <%=t('activerecord.attributes.car_seat.frontseatcenterarmrest')%>:
            </span></dt>
            <dd class="well"><%= @car_seat.frontseatcenterarmrest %></dd>
            <dt class="label-change">
            <span class="label-content label-info">
            <%=t('activerecord.attributes.car_seat.rearseatcenterarmrest')%>:
            </span></dt>
            <dd class="well"><%= @car_seat.rearseatcenterarmrest %></dd>
            <dt class="label-change">
            <span class="label-content label-info">
            <%=t('activerecord.attributes.car_seat.seatventilation')%>:
            </span></dt>
            <dd class="well"><%= @car_seat.seatventilation %></dd>
            <dt class="label-change">
            <span class="label-content label-info">
            <%=t('activerecord.attributes.car_seat.seatheating')%>:
            </span></dt>
            <dd class="well"><%= @car_seat.seatheating %></dd>
            <dt class="label-change">
            <span class="label-content label-info">
            <%=t('activerecord.attributes.car_seat.seatmassage')%>:
            </span></dt>
            <dd class="well"><%= @car_seat.seatmassage %></dd>
            <dt class="label-change">
            <span class="label-content label-info">
            <%=t('activerecord.attributes.car_seat.electricseatmemory')%>:
